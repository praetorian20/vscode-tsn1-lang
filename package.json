{
    "name": "tsn1",
    "displayName": "vscode-tsn1-lang",
    "description": "TSN.1 grammar",
    "version": "0.0.1",
    "engines": {
        "vscode": ">=1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tsn",
                "aliases": [
                    "TSN1",
                    "tsn"
                ],
                "extensions": [
                    ".tsn"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsn",
                "scopeName": "source.tsn",
                "path": "./syntaxes/tsn.tmLanguage.json"
            }
        ]
    }
}
