{
    "name": "tsn",
    "displayName": "vscode-tsn1-lang",
    "description": "TSN.1 grammar",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:tsn",
        "workspaceContains:**/*.tsn"
    ],
    "main": "./out/src/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "tsn",
                "aliases": [
                    "TSN1",
                    "tsn"
                ],
                "extensions": [
                    ".tsn"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsn",
                "scopeName": "source.tsn",
                "path": "./syntaxes/tsn.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.52.0",
        "typescript": "^4.1.3",
        "vsce": "^1.83.0"
    }
}
